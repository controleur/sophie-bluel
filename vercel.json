{
  "version": 2,
  "builds": [
    { "src": "Backend/app.js", "use": "@vercel/node" },
    { "src": "FrontEnd/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "Backend/app.js" },
    { "src": "/images/(.*)", "dest": "Backend/images/$1" },
    { "src": "/(.*)", "dest": "FrontEnd/$1" }
  ]
}
